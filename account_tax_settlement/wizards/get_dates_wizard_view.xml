<odoo>

<record id="view_get_dates_wizard_form" model="ir.ui.view">
     <field name="name">get_dates_wizard</field>
     <field name="model">res.get_dates_wizard</field>
     <field name="arch" type="xml">
        <form string="Set Dates">
            <group>
                <field name="from_date"/>
                <field name="to_date"/>
            </group>
            <footer>
                <button name="confirm" string="Confirm" type="object" class="oe_highlight"/>
                or
                <button string="Cancel" class="oe_link" special="cancel"/>
            </footer>
        </form>
     </field>
 </record>

</odoo>
